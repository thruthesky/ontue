{
  "name": "ontue",
  "version": "1.0.2",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build --prod",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "node app-check.js && ionic-app-scripts serve",
    "publish": "node pwa-check.js && npm run ionic:build -- --aot --prod && node pwa-patch.js && npm run scp",
    "publish:all": "node pwa-check.js && npm run ionic:build -- --prod && node pwa-patch.js && npm run scp:all",
    "serve": "node app-check.js && ionic-app-scripts serve",
    "scp": "cd www && scp -r *.js *.html *.json build ontue@ontue.com:./www",
    "scp:all": "cd www && scp -r * ontue@ontue.com:./www",
    "s": "npm run ionic:serve",
    "pwa": "node pwa-check.js && ionic-app-scripts serve",
    "ionic-pro-build": "node app-check.js && git push ionic master"
  },
  "dependencies": {
    "@angular/common": "4.4.3",
    "@angular/compiler": "4.4.3",
    "@angular/compiler-cli": "4.4.3",
    "@angular/core": "4.4.3",
    "@angular/forms": "4.4.3",
    "@angular/http": "4.4.3",
    "@angular/platform-browser": "4.4.3",
    "@angular/platform-browser-dynamic": "4.4.3",
    "@ionic-native/camera": "^4.3.2",
    "@ionic-native/core": "4.3.0",
    "@ionic-native/file": "^4.5.3",
    "@ionic-native/file-transfer": "^4.5.3",
    "@ionic-native/splash-screen": "4.3.0",
    "@ionic-native/status-bar": "4.3.0",
    "@ionic-native/youtube-video-player": "^4.4.2",
    "@ionic/storage": "2.0.1",
    "@ngx-translate/core": "8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "android-versions": "^1.3.0",
    "ionic-angular": "3.7.1",
    "ionicons": "3.0.0",
    "rxjs": "5.4.3",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.5",
    "@ionic/app-scripts": "^3.1.5",
    "typescript": "2.3.4"
  },
  "description": "An Ionic project"
}
